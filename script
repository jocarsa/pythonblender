import bpy

# Selecciono todos los los objetos
bpy.ops.object.select_all(action='SELECT')

# Y ahora elimino todos los objetos
bpy.ops.object.delete(use_global=False)

# Ahora creo un cubo
bpy.ops.mesh.primitive_cube_add(
    size=2, 
    enter_editmode=False, 
    align='WORLD', 
    location=(0, 0, 0), 
    scale=(1, 1, 1)
    )


